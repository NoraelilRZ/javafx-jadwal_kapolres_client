<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane prefHeight="552.0" prefWidth="808.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.JadwalController">
   <children>
      <GridPane prefHeight="552.0" prefWidth="808.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TableView fx:id="tableJadwal" prefHeight="548.0" prefWidth="808.0" stylesheets="@application.css" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
              <columns>
                <TableColumn fx:id="tanggalCol" prefWidth="${tableJadwal.width*0.1}" sortable="false" style="-fx-alignment: CENTER; -fx-font-size: 20; -fx-font-weight: bold;" text="Tanggal" />
                <TableColumn fx:id="waktuCol" prefWidth="${tableJadwal.width*0.1}" sortable="false" style="-fx-alignment: CENTER; -fx-font-size: 20; -fx-font-weight: bold;" text="Waktu" />
                  <TableColumn fx:id="hariCol" prefWidth="${tableJadwal.width*0.1}" sortable="false" style="-fx-alignment: CENTER; -fx-font-size: 20; -fx-font-weight: bold;" text="Hari" />
                  <TableColumn fx:id="tempatCol" prefWidth="${tableJadwal.width*0.1}" sortable="false" style="-fx-font-size: 20; -fx-font-weight: bold;" text="Tempat" />
                  <TableColumn fx:id="kegiatanCol" prefWidth="${tableJadwal.width*0.3}" sortable="false" style="-fx-font-size: 20; -fx-font-weight: bold;" text="Kegiatan" />
                  <TableColumn fx:id="deskripsiCol" prefWidth="${tableJadwal.width*0.3}" sortable="false" style="-fx-font-size: 20; -fx-font-weight: bold;" text="Disposisi" />
              </columns>
            </TableView>
         </children>
      </GridPane>
   </children>
</AnchorPane>
